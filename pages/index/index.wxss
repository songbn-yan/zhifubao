.count{
    width: 100%;
    height: 450rpx;
    display: block;
    background: #00AAEE;
    border-bottom: 1px solid #E9E9E9;
    padding-bottom: 30rpx;
    position: relative;
}
.count .option{
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100rpx;

}

.count .option .box{
    width: 60%;
    height: 50%;
    margin: 25rpx auto;
}
.count .option .box .principal{
    width: 50%;
    height: 100%;
    font-size: 12px;
    line-height: 50rpx;
    text-align: center;
    float: left;
    letter-spacing: 2px;
    color:#37bdf5;
    border-radius:0;
    border-color:#fff;
}
.count .option .box .interests{
    width: 50%;
    height: 100%;
    font-size: 12px;
    line-height: 50rpx;
    text-align: center;
    float: left;
    letter-spacing: 2px;
    color:#fff;
    border-radius:0;
    border-color:#fff;
}



